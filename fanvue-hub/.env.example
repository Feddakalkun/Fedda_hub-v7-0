# Fanvue OAuth Credentials
# Retrieve these from your Fanvue Creator Dashboard settings
FANVUE_CLIENT_ID=your_client_id_here
FANVUE_CLIENT_SECRET=your_client_secret_here
FANVUE_REDIRECT_URI=http://localhost:3000/api/auth/fanvue/callback

# TikTok OAuth (get these from https://developer.tiktok.com)
TIKTOK_CLIENT_KEY=your_client_key_here
TIKTOK_CLIENT_SECRET=your_client_secret_here
TIKTOK_REDIRECT_URI=http://localhost:3000/api/auth/tiktok/callback

# Clerk Authentication (if using Clerk)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# Database (Prisma)
DATABASE_URL="file:./dev.db"

# Optional: Set defaults for new characters
# NEXT_PUBLIC_CHARACTER_NAME=Emily
